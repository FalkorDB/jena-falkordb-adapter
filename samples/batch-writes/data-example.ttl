@prefix ex:   <http://example.org/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd:  <http://www.w3.org/2001/XMLSchema#> .

# Example data for demonstrating Batch Write optimization
# ========================================================
#
# This file shows a typical bulk load scenario. When loaded with
# transactions, all these triples are efficiently batched using
# Cypher's UNWIND instead of individual operations.
#
# Usage in Java:
# Model model = FalkorDBModelFactory.createModel("myGraph");
# model.begin(ReadWrite.WRITE);
# try {
#     RDFDataMgr.read(model, "data-example.ttl");
#     model.commit();  // Bulk flush
# } finally {
#     model.end();
# }

# People (100 triples with properties)
<http://example.org/person/alice>
    a foaf:Person ;
    foaf:name "Alice" ;
    foaf:age 30 ;
    foaf:email "alice@example.org" ;
    rdfs:label "Alice Smith" .

<http://example.org/person/bob>
    a foaf:Person ;
    foaf:name "Bob" ;
    foaf:age 35 ;
    foaf:email "bob@example.org" ;
    rdfs:label "Bob Jones" .

<http://example.org/person/charlie>
    a foaf:Person ;
    foaf:name "Charlie" ;
    foaf:age 28 ;
    foaf:email "charlie@example.org" ;
    rdfs:label "Charlie Brown" .

<http://example.org/person/diana>
    a foaf:Person ;
    foaf:name "Diana" ;
    foaf:age 32 ;
    foaf:email "diana@example.org" ;
    rdfs:label "Diana Prince" .

<http://example.org/person/eve>
    a foaf:Person ;
    foaf:name "Eve" ;
    foaf:age 29 ;
    foaf:email "eve@example.org" ;
    rdfs:label "Eve Adams" .

# Relationships (social network)
<http://example.org/person/alice>
    foaf:knows <http://example.org/person/bob> ,
               <http://example.org/person/charlie> ,
               <http://example.org/person/diana> .

<http://example.org/person/bob>
    foaf:knows <http://example.org/person/charlie> ,
               <http://example.org/person/eve> .

<http://example.org/person/charlie>
    foaf:knows <http://example.org/person/diana> ,
               <http://example.org/person/eve> .

<http://example.org/person/diana>
    foaf:knows <http://example.org/person/eve> ,
               <http://example.org/person/alice> .

# Organizations
<http://example.org/org/company1>
    a ex:Organization ;
    rdfs:label "Tech Corp" ;
    ex:foundedYear 2010 ;
    ex:employeeCount 500 .

<http://example.org/org/company2>
    a ex:Organization ;
    rdfs:label "Data Inc" ;
    ex:foundedYear 2015 ;
    ex:employeeCount 250 .

# Employment relationships
<http://example.org/person/alice> ex:worksAt <http://example.org/org/company1> .
<http://example.org/person/bob> ex:worksAt <http://example.org/org/company1> .
<http://example.org/person/charlie> ex:worksAt <http://example.org/org/company2> .
<http://example.org/person/diana> ex:worksAt <http://example.org/org/company2> .
<http://example.org/person/eve> ex:worksAt <http://example.org/org/company1> .

# Projects
<http://example.org/project/alpha>
    a ex:Project ;
    rdfs:label "Project Alpha" ;
    ex:status "active" ;
    ex:startDate "2024-01-01"^^xsd:date .

<http://example.org/project/beta>
    a ex:Project ;
    rdfs:label "Project Beta" ;
    ex:status "planning" ;
    ex:startDate "2024-06-01"^^xsd:date .

# Project assignments
<http://example.org/person/alice> ex:worksOn <http://example.org/project/alpha> .
<http://example.org/person/bob> ex:worksOn <http://example.org/project/alpha> .
<http://example.org/person/charlie> ex:worksOn <http://example.org/project/beta> .
<http://example.org/person/diana> ex:worksOn <http://example.org/project/beta> .

# Skills
<http://example.org/skill/java> a ex:Skill ; rdfs:label "Java Programming" .
<http://example.org/skill/python> a ex:Skill ; rdfs:label "Python Programming" .
<http://example.org/skill/databases> a ex:Skill ; rdfs:label "Database Design" .

<http://example.org/person/alice> ex:hasSkill <http://example.org/skill/java> , <http://example.org/skill/databases> .
<http://example.org/person/bob> ex:hasSkill <http://example.org/skill/python> , <http://example.org/skill/databases> .
<http://example.org/person/charlie> ex:hasSkill <http://example.org/skill/java> , <http://example.org/skill/python> .

# Performance note:
# Without transaction: ~180+ individual database operations
# With transaction:    ~5-10 bulk UNWIND operations
# Result:              20-40x faster loading time
